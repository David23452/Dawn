          }
        },
        "row_xWD4LC": {
          "type": "row",
          "settings": {
            "image": "shopify:\/\/shop_images\/Screenshot_9_dcdc62ed-2163-4386-8e79-8c50c9febfa0.jpg",
            "caption": "",
            "heading": "All-in-One Creativity Set üé®",
            "text": "<p>With 8 vibrant watercolors, a refillable brush, and a durable sketchbook, this kit has everything you need to create stunning art on the go‚Äîno bulky supplies required.<\/p>",
            "button_label": "SHOP NOW",
            "button_link": "shopify:\/\/products\/portable-travel-watercolor-set-schmincke-watercolor-8-colors-mini-walnut-paint-box-sketchbook-student-outing-art-supplies"
          }
        },
